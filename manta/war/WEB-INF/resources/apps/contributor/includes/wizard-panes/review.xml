xmlns:ev="http://www.w3.org/2001/xml-events" 
>

	<h2>Review</h2>
	
	<xforms:trigger>
		<xforms:label>&lt; Back</xforms:label>
		<xforms:toggle ev:event="DOMActivate" case="publications"/>
	</xforms:trigger>	
	<xforms:trigger>
		<xforms:label>Confirm</xforms:label>
		<xforms:toggle ev:event="DOMActivate" case="confirmation"/>
	</xforms:trigger>
	
</div>