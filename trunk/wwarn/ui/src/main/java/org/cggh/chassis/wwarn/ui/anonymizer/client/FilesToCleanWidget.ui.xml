<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder 
	xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	ui:generateFormat="com.google.gwt.i18n.rebind.format.PropertiesFormat"
	ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
	ui:generateLocales="default"
	>

	<ui:style>
	
		.fileListHeading {
			margin-top: 0px;
		}
		
		.actionsTableCell {
			text-align: center;
		}

	</ui:style>
	<g:HTMLPanel>
	
		<g:HTMLPanel ui:field="errorPanel">
			<p>
				<g:Label ui:field="errorLabel" />
			</p>
		</g:HTMLPanel>
		<g:HTMLPanel ui:field="pendingPanel">
			<p>
				<ui:msg>Searching for files to clean...</ui:msg>
			</p>
		</g:HTMLPanel>
		<g:HTMLPanel ui:field="filesToCleanPanel">
	
			<h4 class="{style.fileListHeading}">
				<ui:msg>Files to clean:</ui:msg>
			</h4>
	
			<g:Label ui:field="noFilesToCleanLabel">
				<ui:msg>There are no files to clean.</ui:msg>
			</g:Label>
	
			<g:FlowPanel ui:field="filesToCleanTableContainer"/>
	
		</g:HTMLPanel>
		
	</g:HTMLPanel>
</ui:UiBinder>