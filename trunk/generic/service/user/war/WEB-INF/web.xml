<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">

    <display-name>Chassis/* User Service</display-name>

    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>GWTRPCUserServiceServlet</servlet-name>
        <servlet-class>org.cggh.chassis.generic.service.user.gwt.server.RPCUserServiceImpl</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>GWTRPCUserServiceServlet</servlet-name>
        <url-pattern>/gwtrpc/user</url-pattern> <!-- TODO check this will work with GWT -->
    </servlet-mapping>

</web-app>