xquery version "1.0";

module namespace af = "http://www.cggh.org/2010/xquery/atom-format";

declare variable $af:nsuri := "http://www.w3.org/2005/Atom" ;

declare variable $af:feed as xs:string 			:= "feed" ;
declare variable $af:entry as xs:string 		:= "entry" ;
declare variable $af:id as xs:string 			:= "id" ;
declare variable $af:published as xs:string 	:= "published" ;
declare variable $af:updated as xs:string 		:= "updated" ;
declare variable $af:link as xs:string 			:= "link" ;

