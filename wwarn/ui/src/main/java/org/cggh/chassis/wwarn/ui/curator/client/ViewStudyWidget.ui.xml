<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder 
	xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	ui:generateFormat="com.google.gwt.i18n.rebind.format.PropertiesFormat"
	ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
	ui:generateLocales="default"
	>

	<ui:style>
	
		.error {
			color: red;
		}
		
	</ui:style>


	<g:HTMLPanel>

	    <g:HTMLPanel ui:field="errorPanel">
				<g:Label ui:field="errorMessages" />
	    </g:HTMLPanel>

		<g:HTMLPanel ui:field="pendingPanel">
			<g:Label ui:field="pendingMessage">
        		<ui:msg>Pending...</ui:msg>
			</g:Label>
		</g:HTMLPanel>

		<g:HTMLPanel ui:field="contentPanel">

            <h3><ui:msg>ViewStudy</ui:msg></h3>
		</g:HTMLPanel>

	</g:HTMLPanel>

</ui:UiBinder>


