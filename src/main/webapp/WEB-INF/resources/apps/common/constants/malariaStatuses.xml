<!--  A closed list of malaria statuses -->
<!--  used in molecular criteria -->
<malariaStatuses xmlns="">
	<malariaStatus>
		<label>not assessed</label>
		<value>notAssessed</value>
	</malariaStatus>
	<malariaStatus>
		<label>symptomatic only</label>
		<value>symptomatic</value>
	</malariaStatus>
	<malariaStatus>
		<label>asymptomatic only</label>
		<value>asymptomatic</value>
	</malariaStatus>
	<malariaStatus>
		<label>symptomatic and asymptomatic</label>
		<value>symptomaticAndAsymptomatic</value>
	</malariaStatus>
	<malariaStatus>
		<label>severe</label>
		<value>severe</value>
	</malariaStatus>
</malariaStatuses>
